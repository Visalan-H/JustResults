{
  "manifest_version": 3,
  "name": "DOM Data Extractor",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "https://www.mycamu.co.in/*"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "block_all",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.mycamu.co.in/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "inject.js"
      ],
      "matches": [
        "https://www.mycamu.co.in/*"
      ]
    }
  ]
}